{
    "actorSpecification": 1,
    "name": "instagram-comment-lead-intelligence",
    "title": "Instagram Comment Lead Intelligence",
    "description": "Extract qualified leads from Instagram post and reel comments with intent detection and lead scoring.",
    "version": "2.0.0",
    "build": {
        "dockerfile": "./Dockerfile"
    },
    "input": "./input_schema.json",
    "storages": {
        "dataset": {
            "actorSpecification": 1,
            "title": "Instagram Leads",
            "views": {
                "overview": {
                    "title": "All Leads",
                    "transformation": {
                        "fields": [
                            "username",
                            "text",
                            "intent",
                            "intent_score",
                            "leadScore",
                            "is_lead",
                            "postUrl",
                            "likeCount",
                            "postedAt"
                        ]
                    },
                    "display": {
                        "component": "table",
                        "properties": {
                            "username": {
                                "label": "Username",
                                "format": "text"
                            },
                            "text": {
                                "label": "Comment",
                                "format": "text"
                            },
                            "intent": {
                                "label": "Intent",
                                "format": "text"
                            },
                            "intent_score": {
                                "label": "Score",
                                "format": "number"
                            },
                            "leadScore": {
                                "label": "Quality",
                                "format": "text"
                            },
                            "is_lead": {
                                "label": "Lead?",
                                "format": "boolean"
                            },
                            "postUrl": {
                                "label": "Post",
                                "format": "link"
                            },
                            "likeCount": {
                                "label": "Likes",
                                "format": "number"
                            },
                            "postedAt": {
                                "label": "Posted",
                                "format": "date"
                            }
                        }
                    }
                }
            }
        }
    },
    "platform": "node"
}
