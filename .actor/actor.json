{
    "actorSpecification": 1,
    "name": "instagram-comment-lead-intelligence",
    "title": "Instagram Comment Lead Intelligence API",
    "description": "AI-powered lead generation from Instagram comments. Automatically identifies high-intent prospects, scores lead quality, detects purchase intent, filters spam, and extracts contact information. Perfect for B2B SaaS, e-commerce, agencies, and content creators.",
    "version": "1.0.0",
    "meta": {
        "templateId": "instagram-lead-gen"
    },
    "build": {
        "dockerfile": "./Dockerfile"
    },
    "input": "./input_schema.json",
    "storages": {
        "dataset": {
            "actorSpecification": 1,
            "title": "Instagram Leads Dataset",
            "views": {
                "overview": {
                    "title": "Lead Overview",
                    "transformation": {
                        "fields": [
                            "leadId",
                            "username",
                            "fullName",
                            "leadScore",
                            "intent",
                            "comment",
                            "contactInfo.email",
                            "contactInfo.phone",
                            "signals.urgency",
                            "signals.purchaseIntent",
                            "engagement.commentLikes",
                            "timestamp"
                        ]
                    },
                    "display": {
                        "component": "table",
                        "properties": {
                            "leadId": {
                                "label": "Lead ID",
                                "format": "text"
                            },
                            "username": {
                                "label": "Username",
                                "format": "text"
                            },
                            "leadScore": {
                                "label": "Score",
                                "format": "number"
                            },
                            "intent": {
                                "label": "Intent",
                                "format": "text"
                            }
                        }
                    }
                },
                "highScoreLeads": {
                    "title": "High-Score Leads (>0.7)",
                    "transformation": {
                        "fields": [
                            "leadId",
                            "username",
                            "leadScore",
                            "intent",
                            "comment",
                            "contactInfo"
                        ],
                        "limit": 100,
                        "desc": true,
                        "sort": [
                            "leadScore"
                        ]
                    }
                }
            }
        }
    },
    "platform": "node",
    "categories": [
        "SOCIAL_MEDIA",
        "MARKETING",
        "LEAD_GENERATION",
        "BUSINESS_INTELLIGENCE"
    ],
    "tags": [
        "instagram",
        "lead-generation",
        "ai",
        "crm",
        "sales",
        "marketing",
        "intent-detection",
        "spam-filter",
        "lead-scoring",
        "social-media",
        "nlp",
        "openai",
        "gpt-4",
        "b2b",
        "saas",
        "ecommerce"
    ],
    "author": {
        "name": "Your Company Name",
        "email": "support@yourcompany.com",
        "url": "https://yourcompany.com"
    },
    "readme": "./README.md",
    "changelog": "./CHANGELOG.md",
    "icon": {
        "type": "url",
        "value": "https://apify.com/your-org/instagram-comment-lead-intelligence/icon.png"
    },
    "isPublic": true,
    "seoTitle": "Instagram Lead Generation API - AI-Powered Comment Analysis",
    "seoDescription": "Transform Instagram comments into qualified sales leads with AI. Automated lead scoring, intent detection, spam filtering, and contact extraction. Perfect for B2B, SaaS, and e-commerce.",
    "pricingModel": "RENT_PLUS",
    "pricing": {
        "minMonthlyRent": 49,
        "computeUnits": {
            "per1000Comments": 10
        }
    }
